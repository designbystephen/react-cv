{
    "extends": [
      "react-app",
      "airbnb",
      "airbnb-typescript",
      "plugin:jsx-a11y/recommended",
      "prettier",
      "prettier/react"
    ],
    "parserOptions": {
        "project": "./tsconfig.json"
    },
    "plugins": [
      "jsx-a11y",
      "prettier"
    ],
    "rules": {
      "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }]
    }
  }
